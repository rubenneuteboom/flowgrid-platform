# ============================================================================
# Wizard Service - Environment Configuration
# ============================================================================
# Copy this file to .env and fill in your values

# Service Configuration
PORT=3005
NODE_ENV=development

# Database
DATABASE_URL=postgres://flowgrid:your_password@localhost:5432/flowgrid

# Redis (optional, for caching)
REDIS_URL=redis://localhost:6379

# AI Providers
# Primary: Anthropic Claude for agent design
ANTHROPIC_API_KEY=your_anthropic_api_key

# Secondary: OpenAI GPT-4 Vision for image analysis
OPENAI_API_KEY=your_openai_api_key

# AI Provider preference (anthropic/openai)
AI_PROVIDER=anthropic

# Feature Flags (future)
# ENABLE_EXPERIMENTAL_FEATURES=false
# ENABLE_METRICS=true
